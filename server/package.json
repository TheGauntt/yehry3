{
  "name": "server",
  "version": "1.0.0",
  "description": "[TEAM] Whew.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "doc:html": "rimraf docs && typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out docs",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d docs",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepare-release": "run-s reset-hard test doc:html version doc:publish",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "start": "run-s build && node dist/app.js",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "version": "standard-version",
    "watch:test": "nyc --silent ava --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.8.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "typedoc": "^0.19.2"
  }
}
